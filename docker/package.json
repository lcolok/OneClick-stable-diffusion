{
  "name": "@clack/prompts-docker-builders",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src",
    "copy-files": "cpx \"src/**/*.{yaml,json}\" dist",
    "build": "npm run clean && tsc && npm run copy-files && pkg . -t node14 -C gzip --out-path bin && 7z a -t7z -m0=lzma2 -mx=9 -mmt=on ./bin/prompts-docker-builders.7z ./bin/prompts-docker-builders",
    "clean": "del-cli dist"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "app-root-path": "^3.1.0",
    "async-retry": "^1.3.3",
    "cpx": "^1.5.0",
    "i18next": "^22.4.15",
    "js-yaml": "^4.1.0",
    "picocolors": "^1.0.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/i18next": "^13.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.16.1",
    "@types/progress": "^2.0.5",
    "del-cli": "^5.0.0",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "bin": "./dist/index.js"
}
