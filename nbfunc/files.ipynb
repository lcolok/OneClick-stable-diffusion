      "source": [
        "import os\n",
        "\n",
        "def remove_extension(file_name):\n",
        "    _, ext = os.path.splitext(file_name)\n",
        "    while ext:\n",
        "        file_name = os.path.splitext(file_name)[0]\n",
        "        _, ext = os.path.splitext(file_name)\n",
        "    return file_name"
      ]
    }
